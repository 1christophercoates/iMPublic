{
  /* All sensitive local settings should be set in the appSettings.Development.json
    It will have the same structure of this file, but is not checked into source control.
  */
  "ConnectionStrings": {
    "DefaultConnection": "ConnectionStringHolder"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./log.json",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}

